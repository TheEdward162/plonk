; if variants
! PLONK(2): ONE = 1, FOO = =FOO
alloc

	ifeq =ADDR $0 LABEL

	ifeq $0 $ONE BABEL

	ifeq $FOO $A0 CABEL

	ifeq $ONE =ADDR DABEL

	ifeq $A0 R0 EABEL

	ifneq R0 0b11 FABEL

	ifgt =ADDR $0 GABEL
free
!!

; read/write/discard
! PLONK(2): ONE = 1, FOO = =FOO
alloc

	read [=ADDR] NAME
	MOV $NAME, #0b10
	write

	read [$0] FOO
	MOV $FOO, $ONE
	write

	read [$A0]
	MOV $1, $A0
	write

	read [=ADDR]
	TST $1, #0b100
	discard

	read [$FOO]
	MOV $1, #0x1
	write

	read [R0]
	MOV $1, R1
	write

free
!!